<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Loop</title>
</head>
<body>

   <h2>Welcome to Loop</h2>
    <h2>
    <?php
    
    for(
        $count = 0;
        $count < 100;
        $count ++
        )
        {
            echo $count . "</br>";
        }
       
    ?>
    </h2>
</body>
</html>
