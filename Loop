<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Loop</title>
</head>
<body>

   <h2>Welcome to Loop</h2>
    <h2>
    <?php
    
    for(
        $count = 0;
        $count < 11;
        $count ++
        )
        {
            echo $count . ", ";
        }
        echo "</br>";
        echo "</br>";
        for(
        $letter = "a";
        $letter < "z";
        $letter ++
        )
        {
            echo $letter . ", ";
        }
        echo "</br>";
        echo "</br>";
        for(
        $count1 = 0;
        $count1 < 11;
        $count1 ++
        )
        {
            echo $count1 . "</br>";
        }
        
    ?>
    </h2>
</body>
</html>
